<!-- Success Message Flash-->
<% if(success && success.length) { %>
<div
  class="alert custom-flash success alert-dismissible fade show"
  role="alert"
>
  <p><%= success %></p>
  <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
</div>
<% } %>

<!-- Error Message Faliure -->
<% if(error && error.length) { %>
<div class="alert custom-flash error alert-dismissible fade show" role="alert">
  <p><%= error %></p>
  <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
</div>
<% } %>
